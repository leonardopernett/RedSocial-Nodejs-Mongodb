<div class="card">
    <div class="card-header bg-dark text-white">
        <h4><i class="fas fa-fire"></i> most Popular</h4>
    </div>
    <div class="card-body">
        <div class="row">
            {{#each sidebar.popular}}
              <div class="col-md-4">
                  <a href="/image/{{UniqueID}}">
                      <img src="/uploads/{{filename}}" class="img-fluid">
                  </a>
              </div>
            {{/each}}
        </div>
    </div>
</div>